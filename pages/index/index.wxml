
<form bindsubmit="Submit">
  <input name="classname" placeholder="请输入课程名称"></input>
  <input name="teacher" placeholder="请输入教师名称"></input>
  <textarea name="comment" placeholder="请输入评价"></textarea>
  <button type="primary" form-type="submit">提交</button>
  <button  form-type="reset">重置</button>
</form>

<form bindsubmit="Submit2">
<input name="searchname" placeholder="请输入课程名称"></input>
<button type="primary" form-type="submit">搜索</button>
<view wx:for="{{Teacher}}">
  {{item.classname}}-
  {{item.teacher}}
</view>
</form>